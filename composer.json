{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "barryvdh/laravel-cors": "^0.9.3",
        "illuminate/routing": "5.5.2",
        "illuminate/auth": "5.5.2",
        "illuminate/broadcasting": "5.5.2",
        "illuminate/bus": "5.5.2",
        "illuminate/cache": "5.5.2",
        "illuminate/config": "5.5.2",
        "illuminate/container": "5.5.2",
        "illuminate/contracts": "5.5.2",
        "illuminate/database": "5.5.2",
        "illuminate/encryption": "5.5.2",
        "illuminate/events": "5.5.2",
        "illuminate/filesystem": "5.5.2",
        "illuminate/hashing": "5.5.2",
        "illuminate/http": "5.5.2",
        "illuminate/pagination": "5.5.2",
        "illuminate/pipeline": "5.5.2",
        "illuminate/queue": "5.5.2",
        "illuminate/support": "5.5.2",
        "illuminate/translation": "5.5.2",
        "illuminate/validation": "5.5.2",
        "illuminate/view": "5.5.2",
        "kalnoy/nestedset": "^4.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
